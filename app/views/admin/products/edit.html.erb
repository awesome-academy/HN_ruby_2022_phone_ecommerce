<% provide :url, admin_product_path(@product) %>
<div class="row">
  <div class="col-md-12">
    <div class="product-content-right">
      <div class="woocommerce">
        <div class="checkout" ></div>
          <div id="customer_details" class="col2-set flex-center ">
            <div class="col-md-6 .col-md-offset-3">
              <div class="woocommerce-billing-fields">
                <br>
                  <h3><%= t "admin.products.edit.name_view" %></h3>
                <br>
              </div>
            </div>
          </div>
      </div>
    </div>
  </div>
</div>
<%= render "form", product: @product %>

<div class="links-page">
  <%= link_to t("admin.button.show"), @product %>
  |
   <%= link_to t("admin.button.back"), admin_products_path %>
</div>
